{
  "name": "redux-http-client",
  "version": "0.1.0",
  "description": "Pluggable http client using redux for request/response storage",
  "main": "dist/redux-http-client-node.js",
  "browser": "dist/redux-http-client-node.js",
  "author": "Extendi <info@extendi.it>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build:development": "yarn run webpack --mode development",
    "build:production": "yarn run webpack --mode production",
    "test": "yarn run jest --coverage"
  },
  "typings": "./index.d.ts",
  "devDependencies": {
    "@types/jest": "^23.3.0",
    "@types/uuid": "^3.4.3",
    "awesome-typescript-loader": "^5.2.0",
    "jest": "^23.4.1",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^23.0.1",
    "ts-loader": "^4.4.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^2.9.2",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "invariant": "^2.2.4",
    "path-to-regexp": "^2.2.1",
    "redux": "^4.0.0",
    "uuid": "^3.3.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@lib/(.*)$": "<rootDir>/src/$1"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
