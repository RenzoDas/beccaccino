{
  "name": "beccaccino",
  "version": "1.1.0",
  "description": "Easy, reliable and fast http client for redux, framework agnostic, :zap: like a beccaccino",
  "main": "dist/beccaccino-node.js",
  "browser": "dist/beccaccino.js",
  "author": "Extendi <info@extendi.it>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prepublish": "yarn build:production",
    "build:development": "NODE_ENV=development yarn run webpack --progress --mode development",
    "build:production": "rm -rf ./dist/ && NODE_ENV=production yarn run webpack --progress --env.production",
    "test": "yarn run jest --coverage --coverageReporters=text-lcov"
  },
  "files": [
    "dist",
    "index.d.ts"
  ],
  "keywords": [
    "redux",
    "axios",
    "http",
    "client",
    "request",
    "react",
    "vue",
    "angular"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/extendi/beccaccino"
  },
  "typings": "./index.d.ts",
  "devDependencies": {
    "@types/jest": "^23.3.0",
    "@types/uuid": "^3.4.3",
    "awesome-typescript-loader": "^5.2.0",
    "coveralls": "^3.0.2",
    "jest": "^23.4.1",
    "redux": "^4.0.0",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^23.0.1",
    "ts-loader": "^4.4.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^2.9.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "path-to-regexp": "^2.2.1",
    "uuid": "^3.3.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@lib/(.*)$": "<rootDir>/src/$1"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
