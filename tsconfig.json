        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "rootDir": "./",
        "baseUrl": "./src",
        "allowJs": true
    },
    "include": [
        "./src/**/*"
    ]
}
